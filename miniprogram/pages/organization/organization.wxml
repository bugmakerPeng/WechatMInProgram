<form bindsubmit="btnSub">
  <view class="form_">
      <label  class="form_lable">组织部门</label>
      <input name="name" type="text" class="form_input" placeholder="请输入组织部门" ></input>
    </view>
    <view class="form_">
      <label class="form_lable">组织的办公室</label>
      <input name="place" type="text" class="form_input" maxlength="11" placeholder="请输入组织的办公室" ></input>
   </view>
   <view class="form_">
      <label class="form_lable">组织的用品   </label>
      <input name="supplies" type="text" class="form_input" maxlength="11" placeholder="请输入组织的用品 " ></input>
   </view>
   <view class="form_">
      <label class="form_lable">QQ群  </label>
      <input name="QQ_group" type="text" class="form_input" maxlength="11" placeholder="请输入QQ群 " ></input>
   </view>
   
   <view class="form_">
      <label class="form_lable">部门介绍   </label>
      <textarea name="introduction"  class="form_input"  placeholder="请输入部门介绍" ></textarea>
   </view>
<button type="primary" form-type="submit" class="combtn">提交</button>
</form>

 
 









